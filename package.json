{
  "workspaces": {
    "packages": [
      "packages/*",
      "projects/*",
      "setup/*"
    ]
  },
  "scripts": {
    "lint": "npm run lint --workspaces",
    "dev:web-public": "npm run dev --workspace=@njs-five/web-public",
    "build:web-public": "npm run build --workspace=@njs-five/web-public",
    "start:web-public": "npm run start --workspace=@njs-five/web-public",
    "lint:web-public": "npm run lint --workspace=@njs-five/web-public"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/styled-components": "5.1.34",
    "eslint": "9.15.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3"
  }
}