{
  "workspaces": {
    "packages": [
      "packages/*",
      "projects/*",
      "setup/*"
    ]
  },
  "scripts": {
    "lint": "npm run lint --workspaces",
    "dev:web-public": "npm run dev --workspace=@njs-five/web-public",
    "build:web-public": "npm run build --workspace=@njs-five/web-public",
    "start:web-public": "npm run start --workspace=@njs-five/web-public"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/styled-components": "5.1.34",
    "eslint": "8.14.0",
    "prettier": "2.6.2",
    "typescript": "4.5.5"
  }
}